{
  "name": "mentor-management-system-team-4",
  "version": "1.0.0",
  "description": "Mentors Management system is a people management app that enables proper",
  "bin": "bin/createNodejsApp.js",
  "main": "index.js",
  "repository": "https://github.com:ALCOpenSource/Mentor-Management-System-Team-4.git",
  "author": "Martins Aloba",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node index.js",
    "build": "rm -rf ./build/ && babel . --out-dir ./build --ignore .history,node_modules --copy-files && cp .sequelizerc ./build && npm run migrate && npm run seed",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "test:watch": "jest -i --watchAll",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "@babel/register": "^7.21.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^5.0.0",
    "express-session": "^1.17.3",
    "faker": "^5.5.3",
    "form-data": "^4.0.0",
    "helmet": "^4.1.0",
    "http-status": "^1.4.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^9.0.0",
    "mailgun.js": "^8.0.2",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "passport": "^0.6.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "pm2": "^5.1.0",
    "sequelize": "^6.31.0",
    "sequelize-cli": "^6.6.0",
    "uuidv4": "^6.2.13",
    "validator": "^13.0.0",
    "winston": "^3.2.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^26.0.1",
    "lint-staged": "^11.0.0",
    "node-mocks-http": "^1.8.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.0.5",
    "supertest": "^6.0.1"
  }
}
